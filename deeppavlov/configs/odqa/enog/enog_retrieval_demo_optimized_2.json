{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "rank_text_score_id_ensemble"
    ],
    "pipe": [
      {
        "name": "tfidf_ranker",
        "top_n": 500,
        "active": true,
        "in": [
          "question_raw"
        ],
        "out": [
          "doc_ids_500",
          "doc_scores_500"
        ],
        "fit_on_batch": [
          "questions_raw"
        ],
        "vectorizer": {
          "name": "hashing_tfidf_vectorizer",
          "fit_on_batch": [
            "questions_raw"
          ],
          "save_path": "enog/en_ge_book_tfidf.npz",
          "load_path": "enog/en_ge_book_tfidf.npz",
          "tokenizer": {
            "name": "stream_spacy_tokenizer",
            "lemmas": true,
            "ngram_range": [
              1,
              2
            ],
            "stopwords": "sklearn"
          }
        }
      },
      {
        "name": "tfidf_ranker",
        "top_n": 20,
        "active": true,
        "in": [
          "question_raw"
        ],
        "out": [
          "doc_ids",
          "doc_scores"
        ],
        "fit_on_batch": [
          "questions_raw"
        ],
        "vectorizer": {
          "name": "hashing_tfidf_vectorizer",
          "fit_on_batch": [
            "questions_raw"
          ],
          "save_path": "enog/en_ge_book_tfidf.npz",
          "load_path": "enog/en_ge_book_tfidf.npz",
          "tokenizer": {
            "name": "stream_spacy_tokenizer",
            "lemmas": true,
            "ngram_range": [
              1,
              2
            ],
            "stopwords": "sklearn"
          }
        }
      },
      {
        "name": "wiki_sqlite_vocab",
        "id": "my_wiki_sqlite_vocab",
        "in": [
          "doc_ids"
        ],
        "out": [
          "context_raw",
          "doc_ids"
        ],
        "shuffle": false,
        "join_docs": false,
        "load_path": "enog/ge_book.db"
      },
      {
        "ref": "my_wiki_sqlite_vocab",
        "in": [
          "doc_ids_500"
        ],
        "out": [
          "context_raw_500",
          "doc_ids_500"
        ]
      },
      {
        "name": "query_paragraph_tuplifier",
        "in": [
          "question_raw",
          "context_raw_500",
          "doc_ids_500"
        ],
        "out": [
          "question_paragraph_tuples"
        ]
      },
      {
        "name": "tfhub_paragraph_opt_ranker",
        "load_path": "enog/use_vectors.npy",
        "top_n": 20,
        "active": true,
        "encoder": {
          "name": "use_encoder"
        },
        "in": [
          "question_raw"
        ],
        "out": [
          "doc_scores_use",
          "doc_ids_use"
        ]
      },
      {
        "ref": "my_wiki_sqlite_vocab",
        "in": [
          "doc_ids_use"
        ],
        "out": [
          "docs_use",
          "doc_ids_use"
        ]
      },
      {
        "name": "rank_doc_score_tuplifier",
        "id": "my_rank_doc_score_tuplifier",
        "in": [
          "docs_use",
          "doc_scores_use",
          "doc_ids_use"
        ],
        "out": [
          "rank_text_score_id_use"
        ]
      },
      {
        "ref": "my_rank_doc_score_tuplifier",
        "in": [
          "context_raw",
          "doc_scores_500",
          "doc_ids_500"
        ],
        "out": [
          "rank_text_score_id_tfidf"
        ]
      },
      {
        "name": "ensemble_ranker",
        "top_n": 5,
        "in": [
          "rank_text_score_id_tfidf",
          "rank_text_score_id_use"
        ],
        "out": [
          "rank_text_score_id_ensemble"
        ]
      }
    ]
  },
  "train": {
    "validate_best": false,
    "test_best": false,
    "batch_size": 10000
  },
  "metadata": {
    "download": [
      "http://lnsigo.mipt.ru/export/deeppavlov_data/enog.tar.gz"
    ]
  }
}