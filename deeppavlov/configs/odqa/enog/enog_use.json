{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "rank_text_score_id"
    ],
    "pipe": [
      {
        "name": "wiki_sqlite_vocab",
        "in": [
          "question_raw"
        ],
        "out": [
          "context_raw",
          "doc_ids"
        ],
        "shuffle": false,
        "join_docs": false,
        "return_all_content": true,
        "load_path": "enog/ge_book.db"
      },
      {
        "name": "query_paragraph_tuplifier",
        "in": [
          "question_raw",
          "context_raw",
          "doc_ids"
        ],
        "out": [
          "question_paragraph_tuples"
        ]
      },
      {
        "name": "tfhub_paragraph_ranker",
        "top_n": 20,
        "active": true,
        "ranker": {
          "name": "use_sentence_ranker",
          "active": false
        },
        "in": [
          "question_paragraph_tuples"
        ],
        "out": ["docs", "doc_scores", "doc_ids"]
      },
      {
        "name": "rank_doc_score_tuplifier",
        "in": [
          "docs",
          "doc_scores",
          "doc_ids"
        ],
        "out": [
          "rank_text_score_id"
        ]
      }
    ]
  },
  "train": {
    "validate_best": false,
    "test_best": false,
    "batch_size": 100
  },
  "metadata": {
    "download": [
      "http://lnsigo.mipt.ru/export/deeppavlov_data/enog.tar.gz"
    ]
  }
}