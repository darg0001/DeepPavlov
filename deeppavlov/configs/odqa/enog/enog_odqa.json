{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "answer_score_text"
    ],
    "pipe": [
      {
        "config_path": "../deeppavlov/configs/odqa/enog/enog_retrieval_demo_optimized_2.json",
        "in": [
          "question_raw"
        ],
        "out": [
          "rank_text_score_id"
        ]
      },
      {
        "name": "text_extractor",
        "in": [
          "rank_text_score_id"
        ],
        "out": [
          "context_raw",
          "context_scores"
        ]
      },
      {
        "name": "string_multiplier",
        "in": [
          "question_raw",
          "context_raw"
        ],
        "out": [
          "questions"
        ]
      },
      {
        "name": "logit_ranker",
        "squad_model": {
          "config_path": "../deeppavlov/configs/squad/multi_squad_noans.json"
        },
        "top_n": 3,
        "in": [
          "context_raw",
          "questions"
        ],
        "out": [
          "best_answer",
          "context_indices"
        ]
      },
      {
        "name": "context_adder",
        "in": ["best_answer", "context_raw", "context_indices"],
        "out": ["answer_score_text"]
      }
    ]
  },
  "metadata": {
    "download": [
      "http://lnsigo.mipt.ru/export/deeppavlov_data/enog.tar.gz",
      "http://lnsigo.mipt.ru/export/deeppavlov_data/multi_squad_model_noans.tar.gz",
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M.vec",
        "subdir": "embeddings"
      },
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M-char.vec",
        "subdir": "embeddings"
      }
    ]
  }
}
